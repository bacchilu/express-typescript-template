{
  "name": "express-typescript-template",
  "version": "1.0.0",
  "description": "Express TypeScript Template",
  "main": "index.ts",
  "scripts": {
    "build": "rm -rf dict; tsc",
    "start": "node dist/index.js",
    "serve": "concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "update": "ncu -u"
  },
  "author": "Luca Bacchi",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.15",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.6.0",
    "typescript": "^4.9.4"
  }
}
